*{ margin:0px; padding:0px;}

body{ font-family:'PT Sans', Arial, Helvetica, sans-serif; font-size:14px; color:#000; background:#fff url(../images/body.png) 0 0 repeat-x; margin: 0;overflow-x: hidden;}

a {
    color:#00ABA9 ;
}
h1, h2, h3 {
    margin:5px 0;
    color:#000;
}
h1.entry-title, h2.entry-title, h3.entry-title {
    margin:0;
}
.single_lottery_page h2.entry-title {
    background: none repeat scroll 0 0 #00ABA9;
    border-bottom: 5px double #FFFFFF;
    border-top: 5px double #FFFFFF;
    line-height: 31px;
    min-height: 28px;
    padding: 5px 12px 15px;
    text-align: center;
}
.single_lottery_page h2.entry-title a { color:#fff; }

.clear{margin:0px; padding:0px; clear:both;}

.header_top_mid{background:url(../images/header_topmid.png) repeat-x; height:13px; width:100%;}

.header_in {
