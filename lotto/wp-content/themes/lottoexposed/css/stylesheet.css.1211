.subscribe-newsletter { color:#878787;border-bottom: 3px double #aeadad; }

.subscribe-newsletter button {
    display:block;
    width:100%;
    height:40px;
    text-align: center;
    color:#fff;
    line-height: 33px;
    text-transform: uppercase;
    background: #FF2828 ; /* Old browsers */
    /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top, #ff2828 0%, #bd1212 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #ff2828), color-stop(100%, #bd1212)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, #ff2828 0%, #bd1212 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, #ff2828 0%, #bd1212 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top, #ff2828 0%, #bd1212 100%); /* IE10+ */
    background: linear-gradient(to bottom, #ff2828 0%, #bd1212 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1b753d', endColorstr='#00471d',GradientType=0 ); /* IE6-8 */
    border: 1px solid #C40000;
    box-shadow: 0px 1px 1px #fff inset;
    cursor:pointer;
    margin:4px 0 0;
    font-family: Trebuchet MS;
    word-spacing: 4px;
    letter-spacing: 2px;
    font-weight: bold;
    font-size: 16px;
}
.subscribe-newsletter button:hover{
    background: #f42828; /* Old browsers */
    /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top, #f42828 0%, #bd1212 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #f42828), color-stop(100%, #bd1212)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, #f42828 0%, #bd1212 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, #f42828 0%, #bd1212 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top, #f42828 0%, #bd1212 100%); /* IE10+ */
    background: linear-gradient(to bottom, #f42828 0%, #bd1212 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#1b753d', endColorstr='#00471d',GradientType=0 ); /* IE6-8 */
}
