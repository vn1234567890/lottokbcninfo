}
#sidebar-search {
	width:277px;
	height:61px;
	position:relative;
	background: url(../images/search-bg.png) 0 0 no-repeat;
}
#sidebar-search input[type="text"] {
	border:none;
	background: url(../images/search-input.png) 0 0 no-repeat;
	width:155px;
	height:25px;
	padding:0 4px;
	line-height:24px;
	position:absolute;
	left:12px;
	top:8px;
}
#sidebar-search input[type="image"] {
	position:absolute;
	right:12px;
	top:8px;
}
.search .entry-summary {
	margin: 0 25px;
	padding:10px 0 0;
}
.sidebar-block {
	margin: 0 0 20px;
}
.sidebar-block li.widget_text {
	list-style-type:none;
}
.sidebar-block-bg {
	margin: 0 0 20px;
	background: url("../images/sidebar_block.png") 0 0 repeat-y;
	border: 1px solid #CCC;
	padding: 5px 8px;
	border-radius:8px;
	overflow:hidden;
}
.sidebar-block-header {
	background:url("../images/sidebar-block-header.png") 0 0 repeat-x;
	color: white;
	margin: -5px -8px 5px;
	padding: 3px 8px;
	line-height: 24px;
	font-size:15px;
}
.comment-author {
	font-weight: bold;
}
.comment-inside {
