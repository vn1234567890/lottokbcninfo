.blueline {
height: 50px;
background: #2e73b7; /* Old browsers */
background: -moz-linear-gradient(top, #2e73b7 0%, #245ba4 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#2e73b7), color-stop(100%,#245ba4)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top, #2e73b7 0%,#245ba4 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top, #2e73b7 0%,#245ba4 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top, #2e73b7 0%,#245ba4 100%); /* IE10+ */
background: linear-gradient(to bottom, #2e73b7 0%,#245ba4 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#2e73b7', endColorstr='#245ba4',GradientType=0 ); /* IE6-8 */
}

